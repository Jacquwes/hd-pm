            akV_Cap { trigger-keys = <CMMA>; max-prior-idle-ms = <my_cc_term>; bindings = <&kp BSPC &kp LS(V)>; strict-modifiers; };
            akV_t_th { trigger-keys = <G>; max-prior-idle-ms = <my_ak_term>; bindings = <&kp T &kp H>; strict-modifiers; strict-modifiers; };
            akV_t_l { trigger-keys = <M W>; max-prior-idle-ms = <my_ak_term>; bindings = <&kp L>; strict-modifiers; strict-modifiers; };
            akV_b_lv { trigger-keys = <H X>; max-prior-idle-ms = <my_ak_term>; bindings = <&kp BSPC &kp L &kp V>; strict-modifiers; };
        };
        ak_W: ak_W {
