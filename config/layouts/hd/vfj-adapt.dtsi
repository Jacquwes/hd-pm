            akL_Cap { trigger-keys = <CMMA>; max-prior-idle-ms = <my_cc_term>; bindings = <&kp BSPC &kp LS(L)>; strict-modifiers; };
            akW_t_p { trigger-keys = <P LS(P) RS(P)>; max-prior-idle-ms = <my_ak_term>; bindings = <&sl_L>; strict-modifiers; };
        };
        ak_M: ak_M {
            compatible = "zmk,behavior-adaptive-key";
            #binding-cells = <0>;
            bindings = <&kp M>;

            akM_Cap { trigger-keys = <CMMA>; max-prior-idle-ms = <my_cc_term>; bindings = <&kp BSPC &kp LS(M)>; strict-modifiers; };
            akM_t_l { trigger-keys = <G LS(G) RS(G) P X>; max-prior-idle-ms = <my_ak_term>; bindings = <&kp L>; strict-modifiers; };
            akM_b_l { trigger-keys = <W>; max-prior-idle-ms = <my_ak_term>; bindings = <&kp BSPC &kp L &sl_M>; strict-modifiers; };
        };
        ak_N: ak_N {
