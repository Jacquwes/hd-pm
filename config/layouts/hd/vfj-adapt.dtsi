            akT_Cap { trigger-keys = <CMMA>; max-prior-idle-ms = <my_cc_term>; bindings = <&kp BSPC &kp LS(T)>; strict-modifiers; };
            akT_n  { trigger-keys = <K LS(K)>; max-prior-idle-ms = <my_ak_term>; bindings = <&kp N>; strict-modifiers; };
        };
        ak_U: ak_U {
