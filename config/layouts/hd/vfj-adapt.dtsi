            #binding-cells = <0>;
            bindings = <&kp P>;

            akP_Cap { trigger-keys = <CMMA>; max-prior-idle-ms = <my_cc_term>; bindings = <&kp BSPC &kp LS(P)>; strict-modifiers; };
            akP_t_g { trigger-keys = <D>; max-prior-idle-ms = <my_ak_term>; bindings = <&kp G>; strict-modifiers; };
            akP_b_sp { trigger-keys = <F>; max-prior-idle-ms = <my_ak_term>; bindings = <&kp BSPC &kp S &kp P>; strict-modifiers; };
            akP_b_S_P { trigger-keys = <LS(F) RS(F)>; max-prior-idle-ms = <my_ak_term>; bindings = <&kp BSPC &kp LS(S) &kp P>; strict-modifiers; };
        };
        ak_Q: ak_Q {
